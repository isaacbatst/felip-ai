{
  "name": "felip-ai",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "dev": "concurrently -n nest,css -c blue,magenta \"pnpm --filter nest_api start:dev\" \"pnpm --filter nest_api css:watch\"",
    "dev:nest": "pnpm --filter nest_api start:dev",
    "dev:css": "pnpm --filter nest_api css:watch",
    "dev:nest:debug": "pnpm --filter nest_api start:debug",
    "dev:worker": "pnpm --filter tdlib_worker dev",
    "expose": "pnpm --filter nest_api expose",
    "build:nest": "pnpm --filter nest_api build",
    "build:worker": "pnpm --filter tdlib_worker build",
    "build": "pnpm build:nest && pnpm build:worker",
    "db:migrate": "pnpm --filter nest_api db:migrate"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "concurrently": "^9.2.1"
  }
}
